{
  "dashboard": {
    "id": null,
    "title": "Test Service Dashboard",
    "tags": ["test-service", "microservice"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Status",
        "type": "stat",
        "targets": [
          {
            "expr": "test_service_up",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Service Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "test_service_uptime_seconds",
            "legendFormat": "Uptime (seconds)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "test_service_memory_usage_bytes{type=\"rss\"}",
            "legendFormat": "RSS Memory"
          },
          {
            "expr": "test_service_memory_usage_bytes{type=\"heapUsed\"}",
            "legendFormat": "Heap Used"
          },
          {
            "expr": "test_service_memory_usage_bytes{type=\"heapTotal\"}",
            "legendFormat": "Heap Total"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "CPU Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "test_service_cpu_usage_percent",
            "legendFormat": "CPU Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 50 },
                { "color": "red", "value": 80 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 }
      },
      {
        "id": 5,
        "title": "Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(test_service_requests_total{status=\"200\"}[5m])",
            "legendFormat": "Success Requests"
          },
          {
            "expr": "rate(test_service_requests_total{status=\"404\"}[5m])",
            "legendFormat": "Not Found"
          },
          {
            "expr": "rate(test_service_requests_total{status=\"500\"}[5m])",
            "legendFormat": "Server Errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 6, "y": 8 }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "test_service_error_rate",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 2 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
